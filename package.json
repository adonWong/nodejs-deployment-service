{
  "name": "frontend-deployment-service",
  "version": "1.0.0",
  "description": "前端自动化部署服务",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "rimraf dist && tsc",
    "start": "node dist/app.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop deployment-service",
    "pm2:restart": "pm2 restart deployment-service",
    "pm2:logs": "pm2 logs deployment-service"
  },
  "dependencies": {
    "express": "^4.18.2",
    "bull": "^4.12.2",
    "node-ssh": "^13.1.0",
    "axios": "^1.6.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "fs-extra": "^11.1.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.0",
    "@types/cors": "^2.8.15",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.7",
    "@types/fs-extra": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "typescript": "^5.2.0",
    "ts-node-dev": "^2.0.0",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.6",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "deployment",
    "automation",
    "frontend",
    "nodejs",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT"
}